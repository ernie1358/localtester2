{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capability for main, result, and settings windows",
  "windows": ["main", "result", "settings"],
  "permissions": [
    "core:default",
    "core:window:allow-create",
    "core:window:allow-set-focus",
    "core:window:allow-show",
    "core:window:allow-unminimize",
    "core:webview:allow-create-webview-window",
    "core:event:default",
    "opener:default",
    "oauth:default",
    "oauth:allow-start",
    "oauth:allow-cancel",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "updater:default",
    "process:allow-restart"
  ]
}
